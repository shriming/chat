    "test": "npm run lint",
    "postinstall": "./node_modules/bower/bin/bower install",
    "start": "npm run deps",
    "debug": "node debug app.js",
    "init": "npm run deps && npm run make-views",
    "deps": "npm i && ./node_modules/.bin/bower i",
    "make-views": "npm run views-compile && npm run copy-views",
